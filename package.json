{
  "name": "test-driving-lambdas",
  "version": "1.0.0",
  "description": "This project hosts visual studio code server and localstack.",
  "main": "index.js",
  "scripts": {
    "test": "jest ",
    "build": "tsc",
    "localstack": "docker run --rm -d -p 4566:4566 -p 4571:4571 --network code-server-network --network-alias localstack -e LAMBDA_EXECUTOR=docker -v /var/run/docker.sock:/var/run/docker.sock localstack/localstack",
    "predeploy": "cd ./localinfra && LOCALSTACK_HOSTNAME=localstack cdklocal bootstrap -v",
    "deploy": "cd ./localinfra && LOCALSTACK_HOSTNAME=localstack cdklocal deploy --require-approval=never -v",
    "teardown": "cd ./localinfra && LOCALSTACK_HOSTNAME=localstack cdklocal destroy --require-approval=never -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braden-rupp-principal/test-driving-lambdas.git"
  },
  "author": "braden rupp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/braden-rupp-principal/test-driving-lambdas/issues"
  },
  "homepage": "https://github.com/braden-rupp-principal/test-driving-lambdas#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.37",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}